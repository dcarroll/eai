{
    "name": "eai",
    "description": "Einstein.ai plugin",
    "version": "0.0.0",
    "author": "Dave Carroll @dcarroll",
    "bugs": "https://github.com/dcarroll/eai/issues",
    "dependencies": {
        "@oclif/command": "^1",
        "@oclif/config": "^1",
        "@oclif/errors": "^1",
        "@salesforce/command": "^2",
        "@salesforce/core": "^2",
        "base64url": "^3.0.1",
        "fetch": "^1.1.0",
        "form-data": "^3.0.0",
        "jsonwebtoken": "^8.5.1",
        "node-fetch": "^2.6.0",
        "sf-einstein": "^0.0.10",
        "tslib": "^1",
        "yarn": "^1.22.4"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1",
        "@oclif/plugin-help": "^2",
        "@oclif/test": "^1",
        "@salesforce/dev-config": "1.4.1",
        "@types/chai": "^4",
        "@types/mocha": "^5",
        "@types/node": "^10",
        "chai": "^4",
        "globby": "^8",
        "mocha": "^5",
        "nyc": "^14",
        "ts-node": "^8",
        "tslint": "^5"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/lib",
        "/messages",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/dcarroll/eai",
    "keywords": [
        "sfdx-plugin"
    ],
    "license": "MIT",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "sfdx",
        "topics": {
            "eai": {
                "description": "E.ai API Client"
            },
            "eai:auth": {
                "description": "Token management for eai APIs"
            },
            "eai:vision": {
                "description": "Commands to manage vision services"
            },
            "eai:vision:datasets": {
                "description": "Commands to manage vision datasets, topic lists all your datasets"
            }
        },
        "devPlugins": [
            "@oclif/plugin-help"
        ]
    },
    "repository": "dcarroll/eai",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "tslint -p test -t stylish",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif-dev readme && git add README.md"
    }
}